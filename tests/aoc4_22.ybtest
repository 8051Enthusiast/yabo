import std

def for[int] *> range = {
  start: std.basenum(10), ~, end: std.basenum(10)
}

def for[int] *> is_subrange = {
  a: range, ~, b: range, std.newline
  | | std.assert(a.start <= b.start)
      std.assert(a.end >= b.end)
    | std.assert(a.start >= b.start)
      std.assert(a.end <= b.end)
    let return = 1
  | let return = 0
}

export def *test = std.summap(is_subrange)
