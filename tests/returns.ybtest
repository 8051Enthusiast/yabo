def [int] *> u8l = {
    return: ~
}

def [int] *> u16l = {
	lo: ~
	hi: ~
	let return: int = lo + 256 * hi
}

def [int] *> u32l = {
	lo: u16l
	hi: u16l
	let return: int = lo | hi << 16
}

def [int] *> u64l = {
  lo: u32l
  hi: u32l
  let return: int = lo | hi << 32
}

def [int] *> header_magic = {
  n7f: ~
  e: ~
  l: ~
  f: ~
}

