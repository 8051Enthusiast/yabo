import text
import aoc
import math

def *sublistsum = aoc.summap({
  return: text.num?
  text.newline?
})

def *listmax(min: int): int = {
  text.newline?
  sum: sublistsum
  | return: listmax(math.max(min, sum))?
  | let return = min
}

export def *test = {
  first: sublistsum
  return: listmax(first)
}
