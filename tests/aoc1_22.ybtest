import std

def *sublistsum = std.summap({
  return: std.basenum(10)
  std.newline
})

def *listmax(min: int): int = {
  std.newline
  sum: sublistsum
  m: std.max(min, sum)
  | return: listmax(m)
  | let return = m
}

export def *test = {
    first: sublistsum
    return: listmax(first)
}
